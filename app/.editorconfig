# EditorConfig is awesome: https://editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true

[{{*.{json,ts,tsx,js,jsx}}]
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[{package.json,tsconfig.json,.travis.yml}]
indent_style = space
indent_size = 2
